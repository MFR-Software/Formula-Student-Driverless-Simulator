layout {
    pane size=1 borderless=true {
        plugin location="tab-bar"
    }

    pane {
        command "bash"
        args    "./run_fsds_engine.sh"
    }        
    pane {
        command "bash"
        args    "./run_ros2_bridge.sh"
    }
    pane {
        command "bash"
        args    "./run_foxglove.sh"
    }
    
    
    pane size=2 borderless=true {
        plugin location="status-bar"
    }
}

session {
    name "fsds"
}

